<% title 'Currencies'  %>
<br />
<%= link_to image_tag("new.png", :title => "New"), new_currency_path %>
<br /><br />
<table class="list">
  <thead>
  <tr>
    <th align="left">Name</th>
    <th align="left">Code</th>
    <th align="left">Description</th>
    <th>Position</th>
    <th>Action</th>
  </tr>
</thead>
   <tbody>
<% @currencies.each do |currency| %>
  <tr class="<%= cycle("odd", "even") %>">
    <td><%=h currency.name %></td>
    <td><%=h currency.code %></td>
    <td><%=h currency.description %></td>
    <td align="center"><%=h currency.position %></td>
    <td><%= link_to 'Show', currency %>
    <%= link_to 'Edit', edit_currency_path(currency) %>
    <%= link_to 'Destroy', currency, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
   </tbody>
</table>

<br />

