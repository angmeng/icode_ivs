
<% title "Editing Material Category "  %>
<br />
<%= link_to 'Show', @material_category %> |
<%= link_to 'Back', material_categories_path %>
<br /><br/>
<% form_for(@material_category) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

