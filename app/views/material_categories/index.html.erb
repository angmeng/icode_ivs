<% title "Listing Material Categories "  %>
<br />
<%= link_to image_tag("new.png", :title => "New"), new_material_category_path %>
<br />
<% paginated_section @material_categories do %>
<br /><br />
<table class="list">
  <thead>
  <tr>
    <th align="left">Name</th>
    <th align="left">Description</th>
    <th>Action</th>
  </tr>
    </thead>
   <tbody>
<% @material_categories.each do |material_category| %>
    <tr class="<%= cycle("odd", "even") %>">
    <td><%=h material_category.name %></td>
    <td><%=h material_category.description %></td>
    <td align="center"><%= link_to 'Show', material_category %>
    <%= link_to 'Edit', edit_material_category_path(material_category) %>
    <%= link_to 'Destroy', material_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
    </tbody>
</table>
<br />
<% end %>


