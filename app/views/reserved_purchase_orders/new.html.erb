<h1>New reserved_purchase_order</h1>

<% form_for(@reserved_purchase_order) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :purchase_order_number %><br />
    <%= f.text_field :purchase_order_number %>
  </p>
  <p>
    <%= f.label :purchase_order_id %><br />
    <%= f.text_field :purchase_order_id %>
  </p>
  <p>
    <%= f.label :used %><br />
    <%= f.check_box :used %>
  </p>
  <p>
    <%= f.label :remark %><br />
    <%= f.text_area :remark %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', reserved_purchase_orders_path %>