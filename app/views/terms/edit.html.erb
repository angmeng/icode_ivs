<% title "Editing" %>
<br />
<%= link_to 'Show', @term %> |
<%= link_to 'Back', terms_path %>
<br /><br/>
<% form_for(@term) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

