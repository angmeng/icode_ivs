<style type="text/css">
  table {font-size:80%;}

</style>
<h3 style="text-align: center">Return Note</h3>

<table width="100%" style="border: none">
  <tr>
    <td>
      <b>Goods Return Note No</b>
    </td>
    <td>:</td>
    <td><%= @return_note.grn_number %>
    </td>
    <td></td><td></td><td></td>
  </tr>
	<tr>
          <td style="width: 15%"><b>Supplier Code</b> </td>
          <td> : </td>
          <td style="width: 60%"><%= @return_note.supplier.code rescue "-" %></td>
          <td style="width: 10%"><b>Date</b> </td>
          <td> : </td>
          <td><%= @return_note.created_at.strftime("%d-%m-%Y") %></td>
	</tr>
	
	<tr>
          <td><b>Supplier name</b> </td>
          <td> : </td>
          <td> <%= @return_note.supplier.name rescue "-" %></td>
          <td><b>DO(s)</b> </td>
          <td> : </td>
          <td> <%= @return_note.do_number %></td>
	</tr>

        <tr>
          <td><b>Supplier Address</b> </td>
          <td> : </td>
          <td> <%= @return_note.supplier.address rescue "-" %></td>
          <td><b>Contact No</b> </td>
          <td> : </td>
          <td> <%= @return_note.supplier.office_phone %></td>
	</tr>
	
</table>
<br />
<table border="1" style="width: 100%;border-style: solid;border-color: #111113;border-collapse: collapse;" cellpadding="2">
  <tr>
    <th align="left">Material Code</th>
    <th align="left">Category</th>
    <th align="left">Description of Item</th>
    <th align="left">UOM</th>
    <th align="left">Qty</th>
    <th align="left">Remark</th>
  </tr>
	
  <% @items.each do |c|  %>
  <tr>
    <td align="left"><%= c.material.code rescue "-" %></td>
    <td align="left"><%= c.material.material_category.name rescue "-" %></td>
    <td align="left"><%= c.material.description %></td>
    <td align="left"><%= c.material.uom.name rescue "-" %></td>
    <td align="left"><%= "%.3f" % c.quantity %></td>
    <td align="left"><%= c.remark %></td>
  </tr>
  <% end %>
  
</table>
